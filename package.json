{
  "name": "si-reason",
  "private": true,
  "version": "0.0.1",
  "description": "",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/scottcheng/si-reason.git"
  },
  "scripts": {
    "bundle": "esbuild ./src/Index.bs.js --outfile=dist/js/bundle.js --bundle --sourcemap --target=chrome58,firefox57,safari11,edge16 --define:process.env.NODE_ENV='\"development\"'",
    "build-css": "esbuild ./src/Main.bs.js --outfile=public/bundle.js --sourcemap --target=chrome58,firefox57,safari11,edge16 --define:process.env.NODE_ENV='\"development\"'",
    "build-re": "bsb -make-world",
    "start-re": "bsb -make-world -w",
    "clean-re": "bsb -clean-world",
    "watch": "bsb -make-world -w",
    "clean": "bsb -clean-world",
    "webpack": "webpack -w",
    "serve": "webpack serve",
    "build": "npm run build-css && npm run build-re",
    "start": "npm-run-all -p build-css start-re webpack",
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "bsrefmt --in-place */**/*.re */*.re"
  },
  "author": "Scott Cheng <me@scottcheng.com> (http://scottcheng.com/)",
  "bugs": {
    "url": "https://github.com/scottcheng/si-reason/issues"
  },
  "homepage": "https://github.com/scottcheng/si-reason#readme",
  "main": "index.js",
  "keywords": [],
  "dependencies": {
    "@reasonable/react": "git+https://github.com/ReasonableML/reason-react.git",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "devDependencies": {
    "bs-platform": "^9.0.2",
    "css-loader": "^5.2.6",
    "esbuild": "^0.12.14",
    "html-webpack-plugin": "^5.3.2",
    "mini-css-extract-plugin": "^2.1.0",
    "node-sass": "^6.0.1",
    "npm-run-all": "^4.1.5",
    "sass": "^1.35.1",
    "sass-loader": "^12.1.0",
    "style-loader": "^3.0.0",
    "url-loader": "^4.1.1",
    "webpack": "^5.42.0",
    "webpack-cli": "^4.7.2",
    "webpack-dev-server": "^3.11.2"
  }
}
